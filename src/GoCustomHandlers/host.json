{
	"version": "2.0",
	"retry": {
		"strategy": "exponentialBackoff",
		"maxRetryAttempts": 5,
		"delay": "PT1S"
	},
	"healthMonitor": {
		"enabled": true,
		"healthCheckInterval": "00:00:10",
		"healthCheckWindow": "00:05:00",
		"healthCheckThreshold": 6,
		"counterThreshold": 0.80
	},
	"logging": {
		"applicationInsights": {
			"samplingSettings": {
				"isEnabled": true				
			}
		},
		"logLevel": {
			"Host.Triggers.CosmosDB": "Warning"
		}		
	},
	"extensionBundle": {
		"id": "Microsoft.Azure.Functions.ExtensionBundle",
		"version": "[4.*, 5.0.0)"
	},
	"customHandler": {	    
		"description": {
			"defaultExecutablePath": "GoCustomHandlers"
		},
		"enableForwardingHttpRequest":true
	}
}