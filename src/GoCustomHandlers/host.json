{
	"version": "2.0",
	"retry": {
		"strategy": "exponentialBackoff",
		"maxRetryCount": 5,
		"minimumInterval": "00:00:02",
		"maximumInterval": "00:00:30"
	},
	"healthMonitor": {
		"enabled": true,
		"healthCheckInterval": "00:00:10",
		"healthCheckWindow": "00:05:00",
		"healthCheckThreshold": 6,
		"counterThreshold": 0.80
	},
	"cosmosDB": {
		"userAgentSuffix": "MOIMHA-CFP-DEMO"
	},
	"logging": {
		"applicationInsights": {
			"samplingSettings": {
				"isEnabled": true
			}
		},
		"logLevel": {
			"Host.Triggers.CosmosDB": "Debug"
		}
	},
	"extensionBundle": {
		"id": "Microsoft.Azure.Functions.ExtensionBundle",
		"version": "[4.*, 5.0.0)"
	},
	"customHandler": {
		"description": {
			"defaultExecutablePath": "GoCustomHandlers"
		},
		"enableForwardingHttpRequest": true
	}
}